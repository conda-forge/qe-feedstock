From c5373212f2717f916e6ee200aad4197b8e43a901 Mon Sep 17 00:00:00 2001
From: Paolo Giannozzi <p.giannozzi@gmail.com>
Date: Fri, 23 Jul 2021 07:50:50 +0200
Subject: [PATCH] Duplicate name set_asr => set_asr_r (same as set_asr_c in
 PHonon/PH/set_asr_c.f90, for real matrices)

---
 PHonon/Gamma/phcg.f90     | 2 +-
 PHonon/Gamma/solve_ph.f90 | 4 ++--
 2 files changed, 3 insertions(+), 3 deletions(-)

diff --git a/PHonon/Gamma/phcg.f90 b/PHonon/Gamma/phcg.f90
index bef1dfb80..6c7a3291f 100644
--- a/PHonon/Gamma/phcg.f90
+++ b/PHonon/Gamma/phcg.f90
@@ -380,7 +380,7 @@ SUBROUTINE cg_eps0dyn(w2,dynout)
      !
      !   impose asr on the dynamical matrix
      !
-     IF (asr) CALL set_asr(nat,nasr,dyn)
+     IF (asr) CALL set_asr_r(nat,nasr,dyn)
      !
      ! diagonalize the dynamical matrix
      !
diff --git a/PHonon/Gamma/solve_ph.f90 b/PHonon/Gamma/solve_ph.f90
index 41e821ef7..5b51d41f3 100644
--- a/PHonon/Gamma/solve_ph.f90
+++ b/PHonon/Gamma/solve_ph.f90
@@ -144,7 +144,7 @@ SUBROUTINE solve_ph ( )
 END SUBROUTINE solve_ph
 !
 !---------------------------------------------------------------------------
-SUBROUTINE set_asr(nat,nasr,dyn)
+SUBROUTINE set_asr_r(nat,nasr,dyn)
   !---------------------------------------------------------------------------
   !
   ! Impose Acoustic Sum Rule on the dynamical matrix
@@ -172,4 +172,4 @@ SUBROUTINE set_asr(nat,nasr,dyn)
   ENDDO
 
   RETURN
-END SUBROUTINE set_asr
+END SUBROUTINE set_asr_r
